{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "run",
            "type": "python",
            "request": "launch",
            "program": "run.py",
            "args": [
                "https://link.ghostsonthe.net"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "test models",
            "type": "python",
            "request": "launch",
            "program": "test_models.py",
            "args": [
                "--runs=3",
                "--models_dir=test_models",
                "--temps=1.1,1.2,1.3,1.4",
                "--top_ks=6",
                "--top_ps=0.7",
                // "--verbose"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "generate",
            "type": "python",
            "request": "launch",
            "program": "generate/generate.py",
            "args": [
                "--delay=0",
                "--gpt_dir=generate/models/model_03_200",
                "--temp=1.3",
                "--top_k=6",
                "--top_p=0.7",
                "--best_of=2",
                "--stylegan_dir=generate/models",
                "--sound_dir=generate/sounds/mp3+12dB",
                "--prompts_file=generate/prompts.json",
                "--run_length=20",
                "--run_deviation=0.75,1.25",
                "--role_format=[{role}]",
                "--image_string=[image]",
                "--roles=artist,scientist",
                "--base_time=2.0",
                "--letter_time=0.2",
                "--image_time=3.0",
                "--run_time=0",
                "--write_deviation=0.85,1.15",
                "--read_deviation=0.35,0.55",
                "--memory=2",
                // "--runs=5",
                // "--rapid",
                // "--conversation_dir=out/02"
                // "--verbose"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "parse link recording",
            "type": "python",
            "request": "launch",
            "program": "parse_link_recording.py",
            "args": [
                "--filepath=distortion/distortion_data_01.dat",
                "--out=distortion/img",
                "--in_framerate=60",
                "--out_framerate=30",
                "--max=0.4"
            ],
            "console": "integratedTerminal"
        }
    ]
}